{
  "name": "education-flower-pots",
  "version": "0.1.0",
  "description": "Self-education project for learning AngularJS.",
  "dependencies": {
    "angular": "^1.7.8",
    "angular-animate": "^1.7.2",
    "angular-aria": "^1.7.2",
    "angular-material": "^1.1.18",
    "angular-messages": "^1.7.2",
    "angular-route": "^1.7.5"
  },
  "devDependencies": {
    "angular-mocks": "^1.7.5",
    "braces": "^3.0.2",
    "cpx": "^1.5.0",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.3.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-spec-reporter": "0.0.32",
    "protractor": "^5.4.1"
  },
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-*}/**/*\" app/lib -C",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1 ./app/",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "npm test -- --single-run",
    "preupdate-webdriver": "npm install",
    "//": "Do not install the Firefox driver to work around https://github.com/angular/webdriver-manager/issues/303.",
    "update-webdriver": "webdriver-manager update --gecko false",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavlova-iryna-s/education-flower-pots.git"
  },
  "keywords": [
    "angularjs"
  ],
  "author": "Iryna Pavlova",
  "license": "GNU GPLv3",
  "bugs": {
    "url": "https://github.com/pavlova-iryna-s/education-flower-pots/issues"
  },
  "homepage": "https://github.com/pavlova-iryna-s/education-flower-pots#readme"
}
